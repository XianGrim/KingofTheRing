WHEN (betround=4 AND HaveTopPair AND (FlushPossible AND StraightPossible) AND (OneCardFlushPossible or OneCardStraightPossible) AND PotSize<18 AND Opponents<3) Return 130 FORCE
WHEN (betround=4 AND HaveTopPair AND (StraightPossible) AND NOT (OneCardStraightPossible AND FlushPossible) AND PotSize<35 AND Opponents<3) Return 130 FORCE
WHEN (betround=4 AND HaveTopPair AND (FlushPossible) AND NOT (OneCardFlushPossible AND StraightPossible) AND PotSize<35 AND Opponents<3) Return 130 FORCE
WHEN (betround=4 AND HaveTopPair AND PairOnBoard AND HaveTwoPair AND NOT (FlushPossible AND StraightPossible)) Return 130 FORCE
WHEN (betround=4 AND (HaveSecondOverPair or HaveSecondTopPair) AND NOT (HaveFullHouse or OneCardStraightPossible or FlushPossible) AND NOT (RaisesOnFlop AND RaisesOnTurn) AND Opponents=2 AND PotSize<40) Return 130 FORCE
WHEN (betround=4 AND HaveTopPair AND NOT (FlushPossible or OneCardStraightPossible)) Return 130 FORCE


/////////////      150         //////////// CHECK Call ^1/2 Pot LATE BET
WHEN (betround=4 AND HaveFlush AND NOT OneCardFlushPossible AND RaisesOnTurn) Return 150 FORCE
WHEN (betround=4 AND HaveOverPair AND NOT (StraightPossible or FlushPossible) AND PotSize<60) Return 150 FORCE
WHEN (betround=4 AND HaveTwoPair AND NOT (PairOnBoard or OneCardFlushPossible or StraightOnBoard) AND PotSize<15) Return 150 FORCE
WHEN (betround=4 AND HaveTopPair AND NOT (FlushPossible or StraightPossible) AND PotSize<20) Return 150 FORCE
WHEN (betround=4 AND HaveTopPair AND NOT (FlushPossible or OneCardStraightPossible) AND PotSize<20 AND Opponents<3) Return 150 FORCE
WHEN (betround=4 AND HaveSecondTopPair AND RankOfTopPairOnBoard > SecondTopPairRank AND PairOnBoard AND NOT (FlushPossible or TwoPairOnBoard or TripsOnBoard) AND PotSize<15 AND (NoBettingOnFlop or NoBettingOnTurn)) Return 150 FORCE

/////////////      200         //////////// BET Call ^1/3 Pot
WHEN (betround=4 AND (hand$A) AND TwoPairOnBoard AND NOT (StraightPossible or FlushPossible) AND NOT HaveFullHouse AND NoBettingOnTurn AND NOT f$CalledOnFlop AND PotSize<=15) Return 200 FORCE

/////////////      300         //////////// CHECK Call ^1/5 Pot
WHEN (betround=4 AND HaveTwoPair AND NOT (PairOnBoard or OneCardFlushPossible or StraightOnBoard) AND Opponents=1 AND PotSize<50) Return 300 FORCE

/////////////      400         //////////// CHECK Call ^MiniBet
WHEN (betround=4 AND ((hand$A)) AND Overcards>1 AND Opponents=1) Return 400 FORCE


/////////////      500         //////////// CHECK FOLD
WHEN (betround=4 AND HaveUnderPair AND TripsOnBoard) Return 500 FORCE

